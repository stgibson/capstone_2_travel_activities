{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "username",
    "password"
  ],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "minLength": 6,
      "maxLength": 30
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "minLength": 6
    }
  },
  "additionalProperties": false
}